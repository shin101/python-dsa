   "source": [
    "def insertion_sort(arr):\n",
    "    for idx in range(1, len(arr)):\n",
    "        while arr[idx-1] > arr[idx] and idx>0:\n",
    "            arr[idx-1], arr[idx] = arr[idx], arr[idx-1]\n",
    "            idx -=1\n",
    "            \n",
    "    return arr\n",
    "                "
   ]
  },
